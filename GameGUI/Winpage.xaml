<Window x:Class="GameGUI.Winpage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GameGUI"
        mc:Ignorable="d"
        Title="Winpage" Height="450" Width="800">
    <Grid>
        <TextBlock HorizontalAlignment="Center" Margin="0,90,0,0" Text="Tillykke du vandt spillet! :D" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="20"/>
        <Button x:Name="Btn_Replay_Game" Content="Spil igen" HorizontalAlignment="Center" Margin="0,293,0,0" VerticalAlignment="Top" FontSize="20" Click="Btn_Replay_Game_Click"/>
        <TextBlock HorizontalAlignment="Left" Margin="288,192,0,0" Text="Din score:" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <TextBlock x:Name="Txt_Block_Score" HorizontalAlignment="Left" Margin="360,192,0,0" Text="0000" TextWrapping="Wrap" VerticalAlignment="Top" RenderTransformOrigin="2.962,2.561"/>

    </Grid>
</Window>
