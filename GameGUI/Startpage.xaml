<Window x:Class="GameGUI.Startpage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GameGUI"
        mc:Ignorable="d"
        Title="Startpage" Height="450" Width="693">
    <Grid HorizontalAlignment="Center" Width="780">
        <Image HorizontalAlignment="Left" Height="434" VerticalAlignment="Center" Width="720" Source="/hangmanBackground.jpg" Margin="43,0,0,0"/>
        <TextBlock HorizontalAlignment="Center" Margin="0,82,0,0" Text="Velkommen til Galgeleg!!!" TextWrapping="Wrap" VerticalAlignment="Top" Height="90" Width="606" FontFamily="Algerian" FontWeight="Bold" FontSize="40"/>
        <Button x:Name="Btn_startGame" Content="Start spil" HorizontalAlignment="Center" VerticalAlignment="Center" Height="33" Width="94" Click="Btn_startGame_Click"/>
    </Grid>
</Window>
